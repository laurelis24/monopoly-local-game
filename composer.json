{
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "cboden/ratchet": "^0.4.4",
        "php": "^8.4"
    },
    "scripts": {
        "start-server": "php -S 0.0.0.0:8000 -t public",
        "start-socket-server": "php src/App/Server/start-server.php",
        "format": "./vendor/bin/php-cs-fixer fix",
        "migrate": "php ./configurations/migrations.php"
    },

    "autoload":{
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.84"
    }
}
